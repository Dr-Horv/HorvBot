{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es6",
      "esnext"
    ],
    "module": "commonjs",
    "outDir": "./build",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
